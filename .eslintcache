oc/BadgerBase/BadgerBase.tsx",[],"/home/joey/github/abc/cashtab-components-ts/src/utils/badger-helpers.ts",[],"/home/joey/github/abc/cashtab-components-ts/src/utils/currency-helpers.ts",[],"/home/joey/github/abc/cashtab-components-ts/src/atoms/ButtonQR/index.ts",[],"/home/joey/github/abc/cashtab-components-ts/src/components/InvoiceTimer/InvoiceTimer.tsx",[],["124","125"],"/home/joey/github/abc/cashtab-components-ts/src/components/BadgerButton/BadgerButton.tsx",["126","127"],"// @flow\n\nimport * as React from 'react';\nimport styled from 'styled-components';\n\nimport {\n    getCurrencyPreSymbol,\n    formatPriceDisplay,\n    formatAmount,\n} from '../../utils/badger-helpers';\n\n\nimport colors from '../../styles/colors';\n\nimport BadgerBase from '../../hoc/BadgerBase';\n\nimport type {\n    ButtonStates,\n    BadgerBaseProps,\n    ValidCoinTypes,\n} from '../../hoc/BadgerBase';\n\nimport PriceDisplay from '../PriceDisplay';\nimport InvoiceTimer from '../InvoiceTimer';\n\nimport Button from '../../atoms/Button';\nimport ButtonQR from '../../atoms/ButtonQR';\nimport Small from '../../atoms/Small';\nimport Text from '../../atoms/Text';\n\nconst SatoshiText = styled.p`\n    font-size: 12px;\n    margin: 0;\n    display: grid;\n    grid-template-columns: max-content max-content max-content;\n    justify-content: end;\n    grid-gap: 5px;\n    align-items: center;\n`;\n\nconst Outer = styled.div`\n    display: grid;\n    grid-template-columns: max-content;\n`;\n\nconst Wrapper = styled('div')<{ hasBorder?: boolean }>`\n    display: grid;\n    grid-gap: 5px;\n    font-family: sans-serif;\n    grid-template-columns: max-content;\n    grid-template-rows: max-content max-content max-content;\n    color: ${colors.fg500};\n    padding: 6px;\n    border: ${props =>\n        props.hasBorder ? `1px dashed ${colors.brand700}` : 'none'};\n    border-radius: 4px;\n`;\n\nconst PriceText = styled.p`\n    font-family: monospace;\n    font-size: 16px;\n    line-height: 1em;\n    margin: 0;\n    display: grid;\n    grid-gap: 5px;\n    grid-auto-flow: column;\n    justify-content: flex-end;\n    align-items: center;\n`;\n\ninterface invoiceInfoOutputsObjs {\n    token_id: string;\n    send_amounts: string;\n}\n\ninterface invoiceInfoObj {\n    fiatTotal?: number;\n    currency?: string;\n    outputs?: Array<invoiceInfoOutputsObjs>;\n}\n\n// Badger Button Props\ntype Props = BadgerBaseProps & {\n    text?: string;\n\n    showAmount?: boolean;\n    showBorder?: boolean;\n    showQR?: boolean;\n\n    coinSymbol: string;\n    coinDecimals?: number;\n    coinName?: string;\n\n    invoiceInfo?: invoiceInfoObj;\n    invoiceTimeLeftSeconds?: number;\n    invoiceFiat?: number;\n\n    handleClick: Function;\n    step: ButtonStates;\n};\n\nclass BadgerButton extends React.PureComponent<Props> {\n    static defaultProps = {\n        showAmount: true,\n        showBorder: false,\n    };\n\n    render() {\n        const {\n            to,\n            step,\n            handleClick,\n\n            currency,\n            price,\n\n            coinType,\n            coinSymbol,\n            coinDecimals,\n            coinName,\n\n            amount,\n            showAmount,\n\n            text,\n            showBorder,\n            showQR,\n            paymentRequestUrl,\n\n            invoiceInfo,\n            invoiceTimeLeftSeconds,\n            invoiceFiat,\n        } = this.props;\n\n        // buttonPriceDisplay -- handle different cases for BIP70 invoices\n\n        // buttonPriceDisplay if no price, or if a bip70 invoice is set from a server without supported websocket updates\n        let buttonPriceDisplay = <Text>Badger Pay</Text>;\n\n        // buttonPriceDisplay of price set in props and no invoice is set\n        if (price && !paymentRequestUrl) {\n            buttonPriceDisplay = (\n                <Text>\n                    {getCurrencyPreSymbol(currency)} {formatPriceDisplay(price)}\n                    <Small> {currency}</Small>\n                </Text>\n            );\n            // buttonPriceDisplay if valid bip70 invoice with price information is available\n        } else if (paymentRequestUrl && invoiceFiat !== undefined) {\n            buttonPriceDisplay = (\n                <Text>\n                    {getCurrencyPreSymbol(currency)}{' '}\n                    {formatPriceDisplay(invoiceFiat)}\n                    <Small> {currency}</Small>\n                </Text>\n            );\n        }\n\n        let determinedShowAmount = (\n            <PriceDisplay\n                coinType={coinType}\n                price={formatAmount(amount, coinDecimals)}\n                symbol={coinSymbol}\n                name={coinName}\n            />\n        );\n        if (!showAmount) {\n            determinedShowAmount = <React.Fragment></React.Fragment>;\n        } else if (\n            showAmount &&\n            paymentRequestUrl &&\n            (!invoiceInfo || !invoiceInfo.currency)\n        ) {\n            determinedShowAmount = <PriceText>BIP70 Invoice</PriceText>;\n        }\n        return (\n            <Outer>\n                <Wrapper hasBorder={showBorder}>\n                    <Text style={{ textAlign: 'center' }}>{text}</Text>\n                    {showQR ? (\n                        <ButtonQR\n                            amountSatoshis={amount}\n                            toAddress={to}\n                            onClick={handleClick}\n                            step={step}\n                            paymentRequestUrl={paymentRequestUrl}\n                        >\n                            {buttonPriceDisplay}\n                        </ButtonQR>\n                    ) : (\n                        <Button onClick={handleClick} step={step}>\n                            {buttonPriceDisplay}\n                        </Button>\n                    )}\n\n                    {determinedShowAmount}\n\n                    {invoiceTimeLeftSeconds !== undefined && (\n                        <InvoiceTimer\n                            invoiceTimeLeftSeconds={invoiceTimeLeftSeconds}\n                        />\n                    )}\n                </Wrapper>\n            </Outer>\n        );\n    }\n}\n\nexport default BadgerBase(BadgerButton);\n","/home/joey/github/abc/cashtab-components-ts/src/atoms/Button/index.ts",[],["128","129"],"/home/joey/github/abc/cashtab-components-ts/src/components/BadgerBadge/BadgerBadge.tsx",[],"/home/joey/github/abc/cashtab-components-ts/src/hoc/BadgerBase/index.js",[],"/home/joey/github/abc/cashtab-components-ts/src/components/InvoiceTimer/index.ts",[],["130","131"],"/home/joey/github/abc/cashtab-components-ts/src/components/PriceDisplay/PriceDisplay.tsx",[],"/home/joey/github/abc/cashtab-components-ts/src/atoms/Button/Button.tsx",[],"/home/joey/github/abc/cashtab-components-ts/src/atoms/Ticker/Ticker.tsx",[],"/home/joey/github/abc/cashtab-components-ts/src/atoms/ButtonQR/ButtonQR.stories.tsx",["132","133","134"],"/home/joey/github/abc/cashtab-components-ts/src/components/PriceDisplay/index.ts",[],"/home/joey/github/abc/cashtab-components-ts/src/atoms/Button/Button.stories.tsx",[],"/home/joey/github/abc/cashtab-components-ts/src/components/BadgerBadge/BadgerBadge.stories.tsx",[],"/home/joey/github/abc/cashtab-components-ts/src/atoms/ButtonQR/ButtonQR.tsx",[],["135","136"],"/home/joey/github/abc/cashtab-components-ts/src/atoms/Ticker/index.ts",[],"/home/joey/github/abc/cashtab-components-ts/src/components/BadgerButton/BadgerButton.stories.tsx",[],{"ruleId":"137","replacedBy":"138"},{"ruleId":"139","replacedBy":"140"},{"ruleId":"137","replacedBy":"141"},{"ruleId":"139","replacedBy":"142"},{"ruleId":"137","replacedBy":"143"},{"ruleId":"139","replacedBy":"144"},{"ruleId":"137","replacedBy":"145"},{"ruleId":"139","replacedBy":"146"},{"ruleId":"147","severity":1,"message":"148","line":20,"column":5,"nodeType":"149","messageId":"150","endLine":20,"endColumn":19},{"ruleId":"147","severity":1,"message":"151","line":31,"column":7,"n